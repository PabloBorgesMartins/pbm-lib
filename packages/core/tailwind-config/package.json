{
  "name": "@kbm/tailwind-config",
  "version": "1.1.0",
  "source": "./src/index.ts",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf ./dist && rm *.tgz",
    "local": "yarn pack",
    "build": "rollup -c",
    "dev": "yarn build && yarn pack -o %s-%v.tgz"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    }
  },
  "peerDependencies": {
    "tailwindcss": "^3.4.3"
  },
  "sideEffects": false
}
